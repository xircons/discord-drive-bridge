{
  "name": "discord-drive-bridge",
  "version": "1.0.0",
  "description": "Enterprise-grade Discord bot for Google Drive management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "deploy:commands": "ts-node scripts/deploy-commands.ts",
    "test": "jest --testEnvironment=node --detectOpenHandles --forceExit",
    "test:unit": "jest --testPathPattern=unit --testEnvironment=node --detectOpenHandles --forceExit",
    "test:integration": "jest --testPathPattern=integration --testEnvironment=node --detectOpenHandles --forceExit",
    "test:coverage": "jest --coverage --testEnvironment=node --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --testEnvironment=node --detectOpenHandles --forceExit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "db:setup": "./scripts/setup-mysql.sh",
    "db:reset": "knex migrate:rollback --all && knex migrate:latest",
    "docker:build": "docker build -t discord-drive-bot .",
    "docker:run": "docker run -p 3000:3000 discord-drive-bot",
    "docs:serve": "python3 -m http.server 8080 -d docs",
    "docs:build": "echo 'Documentation is ready in docs/ directory'",
    "health:check": "curl -f http://localhost:3000/health || exit 1",
    "metrics:check": "curl -f http://localhost:9090/metrics || exit 1"
  },
  "keywords": [
    "discord",
    "google-drive",
    "bot",
    "file-management",
    "oauth"
  ],
  "author": "Discord Drive Bridge Team",
  "license": "MIT",
  "dependencies": {
    "archiver": "^6.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.5",
    "node-cron": "^3.0.3",
    "prometheus-api-metrics": "^1.0.0",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
